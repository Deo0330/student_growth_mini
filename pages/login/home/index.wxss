page form {
  margin-top: 60rpx;
}

page form input {
  padding:20rpx;
  width: 600rpx;
  height: 2.5em;
  font-size: medium;
  background-color: #dddddd;
  justify-content: center;
  border-radius: 35rpx;
  margin: 20rpx;
}

page button {
  width: 100rpx;
  height: 100rpx;
  border-radius: 50%;
  color: white;
  font-size: 15pt;
  margin-top: 50rpx;
  background-image: linear-gradient(150deg, #659a00, #acd598);
}

page .navi_area {
  margin-top: 150rpx;
  display: flex;
  flex: 1;
  color: #1e50b3;
  font-size: medium;
}

page .navi_area view {
  margin-left: 90rpx;
  margin-right: 90rpx;
}

page .svg-footer {
  margin-top: 90rpx;
  font-size: x-small;
}