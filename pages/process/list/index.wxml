<cu-custom bgColor="bg-gradual-blue" isBack="{{true}}">
  <view slot="backText">返回</view>
  <view slot="content">申请进度</view>
</cu-custom>
<view class="cu-list menu margin-top">
  <view class="cu-item solid-bottom {{modalName=='move-box-'+ index?'move-cur':''}}" wx:for="{{10}}" wx:key="id" bindtouchstart="ListTouchStart" bindtouchmove="ListTouchMove" bindtouchend="ListTouchEnd" data-target="move-box-{{index}}">
    <view class="content padding-sm">
      <view class="text-grey">
        <text class="cuIcon-btn text-olive">挑战杯</text>
        <view class="cu-capsule round">
          <view class="cu-tag bg-blue sm">分数</view>
          <view class="cu-tag line-blue sm">{{index}}</view>
        </view>
      </view>
      <view class="text-cut text-sm radius" style="width:320rpx;">这是一个挑战杯子，这是一个挑战杯子，这是一个挑战杯子，这是一个挑战杯子，这是一个挑战杯子，</view>
    </view>
    <view class="action">
      <view class="text-black">
        <text class="cuIcon-infofill text-red"></text>
        待审核
      </view>
    </view>
    <view class="move">
      <view class="bg-grey" bind:tap="checkDetail" data-item="{{item}}">详情</view>
      <view class="bg-red">取消</view>
    </view>
  </view>
</view>